multi_list_1 = [[1, 3, 10, 9], [8, 2, 4, 9]]
multi_list_2 = [[5, 2, 6, 1], [12, 1, 0, 7]]
sol_list = []
solution = 0
index = 0
jndex = 0
matrix_length = len(multi_list_1[0])
while index < len(multi_list_1):
    sol_list.append([])
    while jndex < matrix_length: 
        solution = multi_list_1[index][jndex] + multi_list_2[index][jndex]
        sol_list[index].append(solution)
        jndex += 1
    jndex = 0
    index +=1 

print(sol_list)


